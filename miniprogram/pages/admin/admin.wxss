/* miniprogram/pages/admin/admin.wxss */
.table {
  border: 1px solid darkgray;
  font-size:40rpx;
 }
 .tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
 }
 .td {
  width:40%;
  justify-content: center;
  text-align: center;
 }
 .td_link {
  width:40%;
  justify-content: center;
  text-align: center;
  color: cornflowerblue;
  text-decoration:underline;
 }
 .td_link2 {
  width:40%;
  justify-content: center;
  text-align: center;
  color: rgb(100, 237, 168);
  text-decoration:underline;
 }

 .td_title{
  width: 70%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;

 }
 
 .bg-w{
  background: snow;
 }
 .bg-g{
  background: #E6F3F9;
 }
 .th {
  width: 40%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
 }
 .agree-p {
  color: cornflowerblue;
  text-decoration:underline;
}