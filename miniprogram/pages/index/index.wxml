<!-- 正方 -->
<view   class="rowmodel" >
  <view class="guideL">
    <text class="headline">正方</text>
    <text class="p1">{{faceTotalNum}}</text>
  </view>

  <!--反方-->
  <view class="guideR">
    <text class="headline">反方</text>
    <text class="p2">{{backTotalNum}}</text>
  </view> 
</view>

<view class="guide" >
    <div class="nav">
      <button class="faceButton"  style="background-image:url('https://7072-prod-f90609-1258198699.tcb.qcloud.la/鱼2.png?sign=940db019b3db585ed57103764ecab487&t=1576058245');"   data-direction='face'  bindtap="onSupport" disabled='{{disableValue}}' ></button>
      <button class="freshButton"  style="background-image:url('https://7072-prod-f90609-1258198699.tcb.qcloud.la/fresh.png');" type="default" bindtap="onCountTotalScore"></button>
      <button class="backButton" style="background-image:url('https://7072-prod-f90609-1258198699.tcb.qcloud.la/熊.png?sign=dad769bc26ad503fb2f8dcdba2e0d380&t=1576058274');"  size="mini" type="default" data-direction='back' bindtap="onSupport" disabled='{{disableValue}}'></button>
    </div>
</view>
